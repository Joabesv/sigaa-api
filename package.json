        "@biomejs/biome": "^1.7.3",
        "@total-typescript/tsconfig": "^1.0.4",
        "@types/cheerio": "^0.22.30",
        "@types/he": "^1.1.2",
        "@types/lodash": "^4.14.172",
        "typedoc": "^0.21.5",
        "typescript": "^5.4.5",
        "unplugin-swc": "^1.4.5",
        "vitest": "^1.6.0"
